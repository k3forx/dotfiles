{
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<Down>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<Right>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<Up>"]
    }
  ],
  "vim.hlsearch": true,
  "files.autoSave": "afterDelay",
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.mac": "/Users/kanata-miyahana/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vscode-kubernetes.kubectl-path.mac": "/Users/kanata-miyahana/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.mac": "/Users/kanata-miyahana/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.formatOnSave": true,
  "cSpell.userWords": ["CSRF", "Vuex", "forx", "tmpl"],
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "tabnine.experimentalAutoImports": true,
  "go.formatOnSave": true,
  "[go]": {
    "editor.insertSpaces": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.tmpl": "html",
    "*.go.tmpl": "html"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.tmpl": "html",
    "*.go.tmpl": "html"
  },
  "hediet.vscode-drawio.local-storage": ""
